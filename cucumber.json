{
  "default": {
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "dryRun": false,
    "paths": [
      "src/test/features/**/*.feature"
    ],
    "require": [
      "src/test/steps/**/*.ts",
      "src/hooks/hooks.ts"
    ],
    "requireModule": [
      "ts-node/register"
    ],
    "format": [
      "progress-bar",
      "html:test-results/cucumber-report.html",
      "json:test-results/cucumber-report.json"
    ]
  },
  "parallel": 3
}